{
  "name": "webpack-react-boilderplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run githooks",
    "githooks": "ln -s -f .githooks/pre-commit .git/hooks/pre-commit ",
    "clean:install": "rm -rf node_modules && npm install",
    "clean:dist": "rm -rf dist",
    "clean:test": "rm -rf .tmp",
    "dist": "npm run clean:dist && npm run test && NODE_ENV=production webpack",
    "start": "webpack-dev-server",
    "test": "npm run lint && npm run clean:test && mocha-webpack -r \"unit-test-setup.js\" --webpack-config webpack.config.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src",
    "playground": "webpack-dev-server"
  },
  "author": "Samuel Nystedt",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.1.0",
    "normalize.css": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^2.0.1",
    "jsdom": "9.4.2",
    "jsdom-global": "2.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.6.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.10.1",
    "precss": "^1.4.0",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.3.3"
  }
}
